{
  "name": "@cryptograss/rabbithole",
  "version": "0.1.0",
  "description": "Discover music by connections, not algorithms",
  "type": "module",
  "main": "dist/rabbithole.js",
  "module": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/rabbithole.cjs"
    },
    "./player": "./src/player/index.js",
    "./state": "./src/state.js",
    "./catalog": "./src/catalog.js",
    "./styles": "./src/styles/rabbithole.css"
  },
  "files": [
    "src/",
