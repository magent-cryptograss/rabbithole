  "repository": {
    "type": "git",
    "url": "https://github.com/cryptograss/rabbithole"
  },
  "bugs": {
    "url": "https://github.com/cryptograss/rabbithole/issues"
  },
  "homepage": "https://justinholmes.com/rabbithole",
  "devDependencies": {
    "eslint": "^8.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "webamp": "^1.5.0"
  },
  "dependencies": {
    "packery": "^3.0.0"
